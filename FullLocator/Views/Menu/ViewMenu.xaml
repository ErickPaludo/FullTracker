<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:pages="clr-namespace:FullLocator.Views"
             xmlns:content="clr-namespace:FullLocator.Views.Menu"
             x:Class="FullLocator.Views.Menu.ViewMenu"
             Title="ViewMenu">

    <FlyoutPage.Flyout>
        <content:ViewContentMenu/>
    </FlyoutPage.Flyout>

    <FlyoutPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <pages:ViewHome/>
            </x:Arguments>
        </NavigationPage>
    </FlyoutPage.Detail>

</FlyoutPage>